{
  "extends": "../tslint.json",
  "rules": {
    "indent": true,
    "eofline": false,
    "variable-name": [
      true,
      "allow-leading-underscore"
    ],
    "directive-selector": [
      true,
      "attribute",
      "app",
      "camelCase"
    ],
    "component-selector": [
      true,
      "element",
      "app",
      "kebab-case"
    ]
  }
}